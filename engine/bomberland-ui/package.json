{
    "name": "@coderone/bomberland-ui",
    "version": "1.0.0",
    "private": true,
    "description": "bomberland-ui",
    "author": "Matthew",
    "keywords": [
        "gatsby"
    ],
    "scripts": {
        "develop": "gatsby develop",
        "start": "gatsby develop",
        "start:local": "env-cmd -f dev.env gatsby develop --host=0.0.0.0 --port=3001",
        "build": "gatsby build",
        "build:prod": "gatsby build && yarn copy",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "copy": "shx cp -r public ../bomberland-engine/public",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@fluentui/react": "^8.65.0",
        "babel-plugin-styled-components": "^2.0.7",
        "gatsby": "^4.12.1",
        "gatsby-image": "3.11.0",
        "gatsby-plugin-image": "^2.7.0",
        "gatsby-plugin-minify": "^0.4.0",
        "gatsby-plugin-react-helmet": "^5.7.0",
        "gatsby-plugin-sharp": "^4.7.0",
        "gatsby-plugin-styled-components": "^5.7.0",
        "gatsby-remark-autolink-headers": "^5.7.0",
        "gatsby-remark-images": "^6.7.0",
        "gatsby-remark-prismjs": "^6.7.0",
        "gatsby-remark-static-images": "^1.2.1",
        "gatsby-source-filesystem": "^4.7.0",
        "gatsby-transformer-remark": "^5.7.0",
        "gatsby-transformer-sharp": "^4.7.0",
        "prismjs": "^1.28.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-helmet": "^6.1.0",
        "react-i18next": "^11.16.6",
        "styled-components": "^5.3.5",
        "use-sound": "^2.0.1"
    },
    "devDependencies": {
        "@types/node": "^17.0.23",
        "@types/react": "^17.0.44",
        "@types/react-dom": "^17.0.15",
        "@types/react-helmet": "^6.1.5",
        "@types/styled-components": "^5.1.25",
        "env-cmd": "^10.1.0",
        "http-proxy-middleware": "^2.0.6",
        "shx": "^0.3.4",
        "typescript": "^4.7.3"
    }
}
